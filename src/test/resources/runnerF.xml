<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="freecrm test">
    <parameter name="useCloudEnv" value="false"></parameter>
    <parameter name="envName" value="browserstack"></parameter>
    <parameter name="os" value="windows"></parameter>
    <parameter name="osversion" value="11"></parameter>
    <parameter name="browservesion" value="111"></parameter>
    <parameter name="browserName" value="chrome"></parameter>
    <parameter name="url" value="https://freecrm.com/"></parameter>

                <test name="login test">
                    <classes>
                        <class name="us.piit.freecrm.LoginTest"></class>
                    </classes>
                </test>

            <test name="Log out test">
                <classes>
                    <class name="us.piit.freecrm.logOutTest"></class>
                </classes>
            </test>
            <test name="sign up test">
                <classes>
                   <class name="us.piit.freecrm.signUpTest"></class>
                 </classes>
               </test>
</suite>
